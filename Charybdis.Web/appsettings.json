{
  "ConnectionStrings": {
    "DefaultConnection": "DataSource=app.db;Cache=Shared"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Authentication": {
    "UserName": "default",
    "Password": "default",
    "IsEnabled": false
  },
  "RateLimiting": {
    "RateLimitWindow": 5,
    "RateLimitSubmissions": 100
  },
  "AppSettings": {
    "ServiceName":  "Charybdis",
    "EnableDetailedErrors": false
  }

}
